<h1>New Category</h1>

<%= form_with(model: @category, local: true) do |form| %>
  

  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <%= form.label :icon %>
  <%= form.collection_select :icon, emojis, :itself, :itself %>

  <%= form.hidden_field :user_id, value: current_user.id %>

  <div class="actions">
    <%= form.submit 'Save' %>
  </div>
<% end %>

<%= link_to 'Home', "/" %>
